{
    "name": "my-chrome-extension",
    "version": "1.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build && powershell \"Compress-Archive -Path dist/* -DestinationPath build_${require('./package.json').version}.zip\"",
        "lint": "vue-cli-service lint",
        "test": "vue-cli-service test:unit tests/components/"
    },
    "dependencies": {
        "aos": "^2.3.4",
        "core-js": "^3.8.3",
        "vue": "^3.2.13",
        "vue-router": "^4.4.3",
        "vuedraggable": "^2.24.3",
        "vuesax": "^4.0.1-alpha.25"
    },
    "devDependencies": {
        "@babel/core": "^7.12.16",
        "@babel/eslint-parser": "^7.12.16",
        "@vue/cli-plugin-babel": "~5.0.0",
        "@vue/cli-plugin-eslint": "~5.0.0",
        "@vue/cli-plugin-unit-jest": "~5.0.0",
        "@vue/cli-service": "~5.0.0",
        "@vue/test-utils": "^2.0.0-0",
        "eslint": "^7.32.0",
        "eslint-plugin-vue": "^8.0.3"
     },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/vue3-essential",
            "eslint:recommended"
        ],
        "parserOptions": {
            "parser": "@babel/eslint-parser"
        },
        "rules": {}
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead",
        "not ie 11"
    ]
}
